@model IEnumerable<Ipt_Project_Website.Models.Resume>

@{
    ViewBag.Title = "SuggestedResume";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SuggestedResume</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Rank No
        </th>
        <th>
            Ranking Score
        </th>
        <th>
            Resume
        </th>
    </tr>

@foreach (var item in ViewBag.Resumes) {

    <tr>
        <td>
            1
        </td>
        <td>
            @item.Value
        </td>
        <td>
            <a href="~/@item.Key">@item.Key</a>
        </td>
    </tr>
}

</table>
